<h1>Update <%=@division.division() %> </h1>

<form action="/divisions/<%= @division.id() %>" method="post">
  <input name="_method" type="hidden" value="patch">
  <label for="description">Redescribe your division:</label>
  <input id="description" name="division" type="text">

  <button type="submit">Update</button>
</form>
